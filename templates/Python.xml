<templateSet group="Python">
  <template name="ase" value="self.assertEqual($EXPECTED$, $ACTUAL$)" description="Assert Equal" toReformat="false" toShortenFQNames="true">
    <variable name="EXPECTED" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="ACTUAL" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="Python" value="true" />
    </context>
  </template>
  <template name="slow" value="@unittest.skipIf(os.environ.get('FAST'), &quot;Only running fast tests.&quot;)" description="Slow test" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="Python" value="true" />
    </context>
  </template>
</templateSet>